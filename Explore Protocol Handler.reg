REGEDIT4

[HKEY_CLASSES_ROOT\Explore]
@="URL:Explore Protocol"
"URL Protocol"=""

[HKEY_CLASSES_ROOT\Explore\DefaultIcon]
@="\"C:\\Windows\\explorer.exe\""

[HKEY_CLASSES_ROOT\Explore\shell]

[HKEY_CLASSES_ROOT\Explore\shell\open]

[HKEY_CLASSES_ROOT\Explore\shell\open\command]
@="cmd /c set url=\"%1\" & call set url=%%url:explore://=%% & call set url=%%url:^%20:= %%  & call start explorer file:%%url%%"
